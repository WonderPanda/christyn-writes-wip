{
  "name": "gatsby-starter-hello-world",
  "description": "Gatsby hello world starter",
  "license": "MIT",
  "scripts": {
    "build:css": "tailwind build ./src/styles/index.tailwind.css -c ./tailwind.config.js -o ./src/styles/index.css",
    "build": "npm run build:css && purgecss -c ./purgecss.config.js -o ./src/layouts && gatsby build",
    "develop": "npm run build:css && gatsby develop",
    "serve": "gatsby serve",
    "graphql-types": "gql-gen --url http://localhost:8000/___graphql --template typescript --out ./src/graphql-types.ts"
  },
  "dependencies": {
    "gatsby": "next",
    "gatsby-plugin-typescript": "^2.0.0-beta.9",
    "gatsby-plugin-typescript-css-modules": "^1.0.4",
    "gatsby-source-filesystem": "^1.5.39",
    "gatsby-transformer-remark": "^1.7.44",
    "lodash": "^4.17.10",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "typeface-karla": "^0.0.54",
    "typeface-open-sans": "^0.0.54"
  },
  "devDependencies": {
    "graphql-code-generator": "^0.5.2",
    "prettier": "^1.14.2",
    "purgecss": "^1.0.1",
    "tailwindcss": "^0.6.5",
    "typescript": "^3.0.1"
  }
}
